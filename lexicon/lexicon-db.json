{
    "$schema": "../_schema/lexicon-db.schema.json",
    "packages": {
        "library": {
            "lib-term": {
                "description": "A terminal library",
                "version": "1.0.0",
                "files": [
                    [
                        "https://raw.githubusercontent.com/alexfayers/cc-24/main/packages/library/lexicon-lib/lib-term.lua",
                        "/usr/lib/lexicon-lib/lib-term.lua"
                    ]
                ],
                "dependencies": []
            },
            "lib-logging": {
                "description": "A simple logging library",
                "version": "1.0.0",
                "files": [
                    [
                        "https://raw.githubusercontent.com/alexfayers/cc-24/main/packages/library/lexicon-lib/lib-logging.lua",
                        "/usr/lib/lexicon-lib/lib-logging.lua"
                    ]
                ],
                "dependencies": [
                    "lib-term"
                ]
            },
            "lib-table": {
                "description": "A table library",
                "version": "1.0.0",
                "files": [
                    [
                        "https://raw.githubusercontent.com/alexfayers/cc-24/main/packages/library/lexicon-lib/lib-table.lua",
                        "/usr/lib/lexicon-lib/lib-table.lua"
                    ]
                ],
                "dependencies": []
            },
            "class-lua": {
                "description": "Make lua Object-Oriented!",
                "version": "1.0.0",
                "files": [
                    [
                        "https://raw.githubusercontent.com/alexfayers/cc-24/main/packages/library/class-lua/class.lua",
                        "/usr/lib/class-lua/class.lua"
                    ]
                ],
                "dependencies": []
            },
            "lib-storage2": {
                "description": "Helper library for the storage2 suite",
                "version": "1.0.0",
                "files": [
                    [
                        "https://raw.githubusercontent.com/alexfayers/cc-24/main/packages/library/lib-storage2/chestHelpers.lua",
                        "/usr/lib/lib-storage2/chestHelpers.lua"
                    ],
                    [
                        "https://raw.githubusercontent.com/alexfayers/cc-24/main/packages/library/lib-storage2/MapSlot.lua",
                        "/usr/lib/lib-storage2/MapSlot.lua"
                    ],
                    [
                        "https://raw.githubusercontent.com/alexfayers/cc-24/main/packages/library/lib-storage2/Map.lua",
                        "/usr/lib/lib-storage2/Map.lua"
                    ],
                    [
                        "https://raw.githubusercontent.com/alexfayers/cc-24/main/packages/library/lib-storage2/Constants.lua",
                        "/usr/lib/lib-storage2/Constants.lua"
                    ],
                    [
                        "https://raw.githubusercontent.com/alexfayers/cc-24/main/packages/library/lib-storage2/Settings.lua",
                        "/usr/lib/lib-storage2/Settings.lua"
                    ],
                    [
                        "https://raw.githubusercontent.com/alexfayers/cc-24/main/packages/library/lib-storage2/helpers.lua",
                        "/usr/lib/lib-storage2/helpers.lua"
                    ]
                ],
                "dependencies": [
                    "lib-logging",
                    "lib-table",
                    "class-lua"
                ]
            },
            "lib-storage2-remote": {
                "description": "Helper library for remote control of storage2",
                "version": "1.0.0",
                "files": [
                    [
                        "https://raw.githubusercontent.com/alexfayers/cc-24/main/packages/library/lib-storage2/remote/enums.lua",
                        "/usr/lib/lib-storage2/remote/enums.lua"
                    ],
                    [
                        "https://raw.githubusercontent.com/alexfayers/cc-24/main/packages/library/lib-storage2/remote/Remote.lua",
                        "/usr/lib/lib-storage2/remote/Remote.lua"
                    ]
                ],
                "dependencies": [
                    "lib-logging",
                    "class-lua"
                ]
            },
            "lib-storage2-remote-client": {
                "description": "Helper library for remote control of storage2 (client)",
                "version": "1.0.0",
                "files": [
                    [
                        "https://raw.githubusercontent.com/alexfayers/cc-24/main/packages/library/lib-storage2/remote/client/Client.lua",
                        "/usr/lib/lib-storage2/remote/client/Client.lua"
                    ],
                    [
                        "https://raw.githubusercontent.com/alexfayers/cc-24/main/packages/library/lib-storage2/remote/client/examplePing.lua",
                        "/usr/lib/lib-storage2/remote/client/examplePing.lua"
                    ]
                ],
                "dependencies": [
                    "lib-logging",
                    "class-lua",
                    "lib-storage2-remote"
                ]
            },
            "lib-storage2-remote-server": {
                "description": "Helper library for remote control of storage2 (server)",
                "version": "1.0.0",
                "files": [
                    [
                        "https://raw.githubusercontent.com/alexfayers/cc-24/main/packages/library/lib-storage2/remote/server/Server.lua",
                        "/usr/lib/lib-storage2/remote/server/Server.lua"
                    ]
                ],
                "dependencies": [
                    "lib-logging",
                    "class-lua",
                    "lib-storage2-remote",
                    "lib-storage2"
                ]
            },
            "lib-turtle": {
                "description": "A library for controlling turtles",
                "version": "1.0.0",
                "files": [
                    [
                        "https://raw.githubusercontent.com/alexfayers/cc-24/main/packages/library/lib-turtle/enums.lua",
                        "/usr/lib/lib-turtle/enums.lua"
                    ],
                    [
                        "https://raw.githubusercontent.com/alexfayers/cc-24/main/packages/library/lib-turtle/helpers.lua",
                        "/usr/lib/lib-turtle/helpers.lua"
                    ],
                    [
                        "https://raw.githubusercontent.com/alexfayers/cc-24/main/packages/library/lib-turtle/Position.lua",
                        "/usr/lib/lib-turtle/Position.lua"
                    ],
                    [
                        "https://raw.githubusercontent.com/alexfayers/cc-24/main/packages/library/lib-turtle/Turtle.lua",
                        "/usr/lib/lib-turtle/Turtle.lua"
                    ],
                    [
                        "https://raw.githubusercontent.com/alexfayers/cc-24/main/packages/library/lib-turtle/TurtleInventory.lua",
                        "/usr/lib/lib-turtle/TurtleInventory.lua"
                    ]
                ],
                "dependencies": [
                    "lib-logging",
                    "lib-table",
                    "class-lua",
                    "lib-storage2-remote-client"
                ]
            },
            "metis-argparse": {
                "description": "A simple argument parser",
                "version": "1.0.0",
                "files": [
                    [
                        "https://raw.githubusercontent.com/SquidDev-CC/metis/refs/heads/dev/src/metis/argparse.lua",
                        "/usr/lib/metis/argparse.lua"
                    ]
                ],
                "dependencies": []
            },
            "lib-discord": {
                "description": "A library for sending notifications to a discord channel",
                "version": "1.0.0",
                "files": [
                    [
                        "https://raw.githubusercontent.com/alexfayers/cc-24/main/packages/library/lib-discord/discord.lua",
                        "/usr/lib/lib-discord/discord.lua"
                    ]
                ],
                "dependencies": []
            }
        },
        "program": {
            "package-example": {
                "description": "A template for creating new programs",
                "version": "1.0.0",
                "files": [
                    [
                        "https://raw.githubusercontent.com/alexfayers/cc-24/main/packages/program/package-example/package-example.lua",
                        "package-example.lua"
                    ]
                ],
                "dependencies": [
                    "lib-logging"
                ],
                "usage": "'package-example'"
            },
            "storage2": {
                "description": "A smart storage system!",
                "version": "1.0.0",
                "files": [
                    [
                        "https://raw.githubusercontent.com/alexfayers/cc-24/main/packages/program/storage2/storage2.lua",
                        "storage2.lua"
                    ],
                    [
                        "https://raw.githubusercontent.com/alexfayers/cc-24/main/packages/program/storage2/storage2-watch.lua",
                        "storage2-watch.lua"
                    ]
                ],
                "dependencies": [
                    "lib-storage2"
                ],
                "usage": "'storage2' and 'storage2-watch'"
            },
            "storage2-server": {
                "description": "Remote control for storage2 (server)",
                "version": "1.0.0",
                "files": [
                    [
                        "https://raw.githubusercontent.com/alexfayers/cc-24/main/packages/program/storage2/storage2-server.lua",
                        "storage2-server.lua"
                    ]
                ],
                "dependencies": [
                    "lib-storage2-remote-server"
                ],
                "usage": "'storage2-server'"
            },
            "quarry": {
                "description": "A simple quarry program",
                "version": "1.0.0",
                "files": [
                    [
                        "https://raw.githubusercontent.com/alexfayers/cc-24/main/packages/program/quarry/quarry.lua",
                        "quarry.lua"
                    ]
                ],
                "dependencies": [
                    "lib-turtle",
                    "metis-argparse",
                    "lib-discord"
                ],
                "usage": "'quarry'"
            }
        }
    }
}